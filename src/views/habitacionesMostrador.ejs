<!DOCTYPE html> 
<html> 
<!-- HEAD -->
<%- include('./partials/head') %>
<body>
    <!-- HEADER -->
    <%- include('./partials/header') %>
    <!-- MAIN -->
<main class="mostrador">
    <!--Slider bootstrap-->

    <section class="carrusel_bootstrap">
        <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="true">
            <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="1" aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="2" aria-label="Slide 3"></button>
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="3" aria-label="Slide 4"></button>
                <button type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide-to="4" aria-label="Slide 5"></button>
            </div>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="/images/habitaciones/Premium/premium1.jpg" class="d-block w-100" alt="1">
                </div>
                <div class="carousel-item">
                    <img src="/images/habitaciones/Ejecutiva/ejecutiva2.jpg" class="d-block w-100" alt="2">
                </div>
                <div class="carousel-item">
                    <img src="/images/habitaciones/Standar/standar1.jpg" class="d-block w-100" alt="3">
                </div>
                <div class="carousel-item">
                    <img src="/images/desayuno/desayuno2.jpg" class="d-block w-100" alt="4">
                </div>
                <div class="carousel-item">
                    <img src="/images/desayuno/desayuno1.jpg" class="d-block w-100" alt="5">
                </div>
            </div>
            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
            </button>
        </div> 
    </section>
 
    <!--End slider--> 
   
    <!--Card-->
    <section class="section-card-room-mostrador">
        
        <h1 class="h1Mostrador">Habitaciones amplias, cómodas y bien equipadas</h1>

        <!--Premium-->
        <a href="/habitaciones/premium/">
            <article class="card-room">
                <div class="photo-room">
                    <img src="/images/habitaciones/Premium/premium8.jpg"alt="Img Premium">
                </div>
                <div class="info-room">
                    <h2>Premium</h2>
                    <div class="description-room">
                        <p class="service-room">
                            Cuenta con una cama King zise, opcional doble matrimonial.
                            internet Wifi, TV pantalla plana, baño privado, aire acondicionado frío/calor, desayuno incluido. 
                            <br><strong>Ver más...</strong>
                        </p>
                        
                        
                        
                        <div class="room">
                            <div class="passenger">
                                <i class="fa-solid fa-user"></i>
                                <i class="fa-solid fa-user"></i>
                                <i class="fa-solid fa-user"></i>
                                <i class="fa-solid fa-user"></i>
                                <i class="fa-solid fa-bed"></i>
                                <i class="fa-solid fa-bed"></i>
                            </div>
                        </div>
                    </div>
                    <div class="room-price">
                        Desde $22.000
                    </div>
                </div>
            </article>
        </a>

        <!--Ejecutiva-->
        <a href="/habitaciones/suite/">
            <article class="card-room">
                <div class="photo-room">
                    <img src="/images/habitaciones/Ejecutiva/ejecutiva7.jpg"alt="Img Premium">
                </div>
                <div class="info-room">
                    <h2>Ejecutiva</h2>
                    <div class="description-room">
                        <p class="service-room">
                            Cuenta con una cama Queen zise, opcional triple matrimonial, triple twin.
                            internet Wifi, TV pantalla plana, baño privado, aire acondicionado frío/calor, desayuno incluido. 
                            <br><strong>Ver más...</strong>
                        </p>
                        <div class="room">
                            <div class="passenger">
                                <i class="fa-solid fa-user"></i>
                                <i class="fa-solid fa-user"></i>
                                <i class="fa-solid fa-user"></i>
                                <i class="fa-solid fa-bed"></i>
                                <i class="fa-solid fa-bed"></i>
                                <i class="fa-solid fa-bed"></i>
                            </div>
                        </div>
                    </div>
                    <div class="room-price">
                        Desde $13.500
                    </div>
                </div>
            </article>
        </a>

        <!--Standar-->
        <a href="/habitaciones/standar/"><article class="card-room">
            <div class="photo-room">
                <img src="/images/habitaciones/Standar/standar8.jpg"alt="Img Premium">
            </div>
            <div class="info-room">
                <h2>Standard</h2>
                <div class="description-room">
                    <p class="service-room">
                        Cuenta con tres camas individuales, opcional triple matrimonial.
                        internet Wifi, TV pantalla plana, baño privado, aire acondicionado frío/calor, desayuno incluido. 
                        <br><strong>Ver más...</strong>
                    </p>
                    <div class="room">
                        <div class="passenger">
                            <i class="fa-solid fa-user"></i>
                            <i class="fa-solid fa-user"></i>
                            <i class="fa-solid fa-user"></i>
                            <i class="fa-solid fa-bed"></i>
                            <i class="fa-solid fa-bed"></i>
                            <i class="fa-solid fa-bed"></i>
                        </div>
                    </div>
                </div>
                <div class="room-price">
                    Desde $8.500
                </div>
            </div>
        </article></a>
        <article class="reserva">
            <a href="https://newstayplace.com/es/o/hotel-terra-patagonia,562su.html?vm=amp" target="_blank"><h2>Hace tu reserva</h2><i class="fa-solid fa-arrow-up-right-from-square"></i></a>
        </article>
        

    <!--End Card-->
        
        
      
    
    
    
    </main>
    <!-- FOOTER -->
<%- include('./partials/footer') %>
</body>
</html>